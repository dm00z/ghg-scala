{
  "name": "ghg",
  "version": "0.2.0",
  "main": "main.js",
  "license": "yar",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dm00z/ghg-scala.git"
  },
  "scripts": {
    "start": "concurrently \"webpack --watch\" \"webpack-dev-server --progress --colors --port 8090\"",
    "build": "webpack --config webpack.config.prod.js",
    "dist": "electron-builder",
    "release": "electron-builder"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "electron": "^2.0.9",
    "electron-builder": "^20.28.4",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "run-sequence": "^1.2.2",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "@bower_components/MathJax": "mathjax/MathJax#~2.7.1",
    "d3": "^3.5.12",
    "electron-updater": "^3.1.2",
    "gulp-atom-electron": "^1.17.1",
    "material-design-lite": "^1.3.0",
    "material-ui": "^0.14.0",
    "materialize-css": "0.100.2",
    "moment": "^2.10.6",
    "react": "^0.14.5",
    "react-addons-update": "^0.14.5",
    "react-d3-basic": "^1.5.2",
    "react-dom": "^0.14.5",
    "react-faux-dom": "^2.1.1",
    "react-tap-event-plugin": "^0.2.1"
  },
  "build": {
    "appId": "com.dm00z.mthmodel",
    "productName": "MTH Model",
    "files": [
      "assets/**/*",
      "mui/**/*",
      "bower_components/**/*",
      "package.json",
      "main.js",
      "index.html"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "publish": [
        {
          "provider": "github",
          "owner": "dm00z",
          "host": "github.com",
          "protocol": "https"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dir"
      ]
    }
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
